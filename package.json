{
  "name": "time-chart-widget",
  "packageManager": "pnpm@10.8.1",
  "description": "事件选择小部件",
  "license": "MIT",
  "scripts": {
    "start": "rspress dev",
    "docs:build": "rspress build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "biome check --write",
    "release:build": "pnpm run --filter @sensoro-design/* build",
    "release:latest": "changeset pre exit && changeset add",
    "release:next-alpha": "changeset pre enter alpha",
    "release:next-beta": "changeset pre enter beta",
    "release:next-rc": "changeset pre enter rc",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:e2e": "playwright test",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "simple-git-hooks",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "d3-array": "^3.2.4",
    "d3-brush": "^3.0.0",
    "d3-selection": "^3.0.0",
    "es-toolkit": "^1.35.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint-react/eslint-plugin": "^1.47.2",
    "@playwright/test": "^1.51.1",
    "@rcuse/core": "^0.25.0",
    "@rsbuild/core": "^1.3.6",
    "@rsbuild/plugin-react": "^1.2.0",
    "@rslib/core": "^0.6.3",
    "@rspress/plugin-playground": "^1.43.11",
    "@rspress/plugin-preview": "^1.43.11",
    "@rspress/plugin-typedoc": "^1.43.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/d3-array": "^3.2.1",
    "@types/d3-brush": "^3.0.6",
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "dayjs": "^1.11.13",
    "eslint": "^9.24.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.1",
    "nx": "^20.7.2",
    "playwright": "^1.51.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rspress": "^1.43.11",
    "simple-git-hooks": "^2.12.1",
    "typescript": "~5.8.2",
    "vitest": "^3.1.1"
  },
  "simple-git-hooks": {
    "commit-msg": "npx --no -- commitlint --edit $1",
    "pre-commit": "npx lint-staged",
    "preserveUnused": [
      "commit-msg"
    ]
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
